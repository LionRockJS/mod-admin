{
  "wrapper": "div.container.mx-auto>div.w-full.md:w-\\[545px\\].bg-paper.border-x.border-black-500.min-h-\\[100vh\\].px-\\[24px\\].md:px-\\[55px\\].py-\\[144px\\]",
  "sections": {
    "signup": {
      "type": "card-signup",
      "settings": {
        "enabled": "{{allowSignup}}"
      }
    },
    "login": {
      "type": "card-login",
      "settings": {
        "enabled": "{% if allowSignup == false %}true{% endif %}"
      }
    }
  },
  "order": [
    "signup",
    "login"
  ]
}