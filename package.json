  },
  "homepage": "https://github.com/LionRockJS/mod-admin",
  "dependencies": {
    "camelize": "^1.0.1",
    "csv-parse": "^5.5.6",
    "decamelize": "^6.0.0",
    "moment": "^2.30.1"
